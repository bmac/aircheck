<h1>{{ roomName }}</h1>
<section class="others clearfix">
    <div>Other Videos: {{ otherVideos.length }}</div>
    {{#each otherVideos}}
    <div class="pull-left other-group">
        <label class="other-label">{{ socketId }}</label>
        <video {{bindAttr src=videoSrc }} controls class="other"></video>
    </div>
    {{/each}}
    <div class="clearfix"></div>
</section>
<section class="you">
    <video {{bindAttr src=myStream.videoSrc }} controls id="video"></video>
</section>
<section class="messages">
{{input type="text" value=message class="msg" action="sendMessage" onEvent="enter" }}
{{#each messages}}
    <div >
        {{ time }} &lt;{{nick}}&gt; {{msg}}
    </div>
    {{/each}}
</section>
